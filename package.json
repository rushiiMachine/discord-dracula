{
	"name": "discord-dracula",
	"description": "cute dracula theme for discord",
	"type": "module",
	"private": "true",
	"scripts": {
		"bench": "pnpm dev && node ./scripts/remoteBenchmark.js",
		"build": "pnpm lint && node ./scripts/build.js",
		"dev": "node ./scripts/dev.js",
		"launchDiscord": "node ./scripts/launchDiscord.js",
		"lint": "stylelint \"./src/**.scss\"",
		"lint:fix": "pnpm lint --fix",
		"watch": "pnpm dev --watch"
	},
	"devDependencies": {
		"@types/chrome-remote-interface": "^0.31.14",
		"@types/node": "^22.14.1",
		"chalk": "^5.4.1",
		"chokidar": "^4.0.3",
		"chrome-remote-interface": "^0.33.3",
		"cli-table3": "^0.6.5",
		"csv-parse": "^5.6.0",
		"dotenv": "^16.5.0",
		"sass": "^1.86.3",
		"stylelint": "^16.18.0",
		"stylelint-config-standard-scss": "^14.0.0"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher"
		]
	},
	"packageManager": "pnpm@10.12.1"
}
